{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Linux Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "EK-TM4C123GXL-custom",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "executable": "${command:cmake.launchTargetPath}",
      "type": "cortex-debug",
      "servertype": "jlink",
      "interface": "swd",
      "device": "TM4C123GH6PM"
    },
  ]
}
